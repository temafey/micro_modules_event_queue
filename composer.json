{
    "name": "micro-module/event-queue",
    "type": "library",
    "description": "Micro module Event Queue library",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "beberlei/assert": "^3.3",
        "micro-module/broadway": "^2.3",
        "micro-module/broadway-bundle": "^0.6",
        "micro-module/enqueue": "^0.10",
        "micro-module/enqueue-fs": "^0.10",
        "micro-module/job-queue": "^0.10 || ^0.11",
        "micro-module/base": "^0.9",
        "micro-module/command-bus": "^0.5",
        "psr/log": "^1.1 || ^2.0 || ^3.0",
        "queue-interop/amqp-interop": "^0.8",
        "queue-interop/queue-interop": "^0.7 || ^0.8"
    },
    "require-dev": {
        "micro-module/null": "^0.11",
        "php-parallel-lint/php-console-highlighter": "^1.0",
        "php-parallel-lint/php-parallel-lint": "^1.4",
        "mockery/mockery": "^1.6",
        "phpmd/phpmd": "^2.15",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-mockery": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^10.0 || ^11.0",
        "roave/security-advisories": "dev-latest",
        "symplify/easy-coding-standard": "^12.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "MicroModule\\EventQueue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MicroModule\\EventQueue\\Tests\\Unit\\": "tests/unit"
        }
    }
}
